/*
 * Unofficial douban.com skin
 *
 * Author      : furikuri
 * Created     : Dec 18th, 2007
 * Updated     : Dec 18th, 2007
 * Version     : 1.0
 * Website     : http://pouringthemilkshake.org,http://www.douban.com/people/flcl/
 * Description : Modifies the home page for douban
 * Usage       : Use with Stylish Firefox extension (http://userstyles.org/) or copy to your Firefox userContent.css file
 */
/* --------------------------------------------------------------- */


/* BUGS  --------------------------------------------------------- */
/* Please send me any bugs! Douyou is always best,or comment the topic*/
/* --------------------------------------------------------------- */

@namespace url(http://www.w3.org/1999/xhtml);

@-moz-document domain("douban.com") {
body {
color: #f26d7d !important;
background: #0b060d url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/Fingers_purple_top.jpg) repeat-x top left !important;
}
body a ,#maxw #ssform #header #status span ,.rlnk {
color:#ED145A !important;
text-decoration:none !important;
}
body a:hover ,#maxw #ssform #header #status span:hover {
text-decoration:underline !important;
background:#0F0810 !important;
}
.fdiv:after {
content:"Current skin:'Douban Zilla' by furikuri,-- send me a douyou or comment the topic if you find a bug or want something changed."!important;
display: block !important;
color: #ed145a !important;
text-align:center !important;
}
#maxw {
max-width:3000px !important;
background:transparent url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/Fingers_bottom.jpg) repeat-x scroll  bottom !important;
}
#maxw #nav a:hover {
background:#050305 url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/flame_block_z.gif) repeat scroll left bottom !important;
}
#maxw #nav .now span {
background:transparent !important;
}
#maxw #ssform #header #status ,#maxw #nav a {
background:#050305 !important;
}
#maxw #ssform #header #status {
-moz-border-radius:5px !important;
margin-top:-5px !important;
}

#page_focus {
border:1px solid #ED145A !important;
-moz-border-radius:5px !important;
width:200px !important;
}

.gact {color:#0B060D !important;border:#0B060D !important;}

a.redbutt ,a.redbutt:hover {
background:url(redbutt.gif) !important;
border:1px solid #D51253 !important;
-moz-border-radius:5px !important;
}
a.redbutt span {background:url(redbutt.gif) !important;}

.tablelt ,.tablect ,.tablert ,.tablelc ,.tablecc ,.tablerc ,.tablelb ,.tablecb ,.tablerb ,.sbjtd ,.zbar ,.zbar div ,.zbar span.now ,.zbar span.now span
{background:#170B1C !important;}
#maxw #main #tablerb .mbt .mbtr {border-bottom:0 !important;}

#maxw #main #tablerb .obs .gact a {
border:0px solid #0B060D !important;
}

.mt {border-bottom:20px solid #0B060D !important;}
.mmp {color:#E91357 !important;}
.olt td ,.bs li ,.mbtr ,.ninetaps h1{
border-bottom:1px solid #E26675 !important;
}
.mbdiv ,.usernav a ,.fdiv {
border-top:1px solid #E26675 !important;
}
.ul ,.indent span {
border-bottom:1px solid #0B060D !important;
}
.mt .indent .indent span {border-bottom:#0B060D !important;}


#maxw .fdiv {
border-top:0 !important;
margin-bottom:110px !important;
}
#maxw .fdiv .fspl {
color: #f26d7d !important;
}
#maxw .fdiv .fspr {
color:#DD145A !important;
}
/*h*/
h1 ,h2 ,.m ,.pl2 ,.mn{
color:#64706D !important;
}
.pl {color:#ED145A !important;}
h3 ,h4 ,.wrap ,.xbar div ,.paginator ,.xbar span.now ,.xbar span.now span{
background:transparent url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/menu_strip.jpg) repeat-x scroll !important;
width:105% !important;
}
h3 {
-moz-border-radius:5px !important;
margin:0px 0pt 0px -10px !important;
width:105% !important;
}
/*h4 {margin:0px 0px 0px -30px !important;}*/
/*textarea*/
textarea {
color:#E26675 !important;
background:#2E1125 !important;
border:3px solid #8E364B !important;
}

input[type=submit] {color:#E26675 !important;background:transparent !important;border:1px solid #8E364B !important;-moz-border-radius:6px !important;}
input[type=title] {color:#E26675 !important;}
input[type=text] ,input[type=password] {color:#E26675 !important;background:#2E1125 !important;border:1px solid #8E364B !important;-moz-border-radius:6px !important;width:300px !important;height:20px !important;}


input:hover[type=button],
input:hover[class=button],
input:hover[type=submit] {background:#050305 url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/flame_block_z.gif) repeat scroll left bottom !important;}


/*9*/
.gact a, a.gact {
border-color:#ED145A !important;
-moz-border-radius:6px !important;
}
.pl span {color:#D61F5A !important;}



/*img*/
img[src="/pics/logosmall.gif"]{
background:url(http://pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/doubanlogo.png) !important;
width:0 !important;
padding:17px 82px !important;
}
img[src="/pics/douban9.gif"]{
background:url(http://pouringthemilkshake.org/images/douban/deviantART%3EZilla/douban9.png) !important;
width:0 !important;
height:0 !important;
padding:25px 45px !important;
}
img[src="/pics/down.gif"]{
background:url(nsts.gif) !important;
width:0 !important;
}
img[src="/pics/new1.gif"]{
background:url(nsts.gif) !important;
width:0 !important;
}
#searbar span.submit {background:url(/search.gif) !important;background:#2E1125 !important;border:1px solid #ED145A !important;-moz-border-radius:6px !important;}
#searbar span.submit:hover ,#searbar span.submit .butt:hover {background:transparent url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/flame_block_z.gif) repeat scroll left bottom !important;}


h3 img {
border:1px solid #ED145A !important;
-moz-border-radius:5px !important;
}
img[src="/pics/arrow0.gif"],img[src="/pics/arrow1.gif"],img[src="/pics/arrow2.gif"],img[src="/pics/arrow3.gif"]{
width:0px !important;
padding:11px 30px 11px 30px !important;
}
img[src="/pics/arrow0.gif"]:hover,img[src="/pics/arrow1.gif"]:hover,img[src="/pics/arrow2.gif"]:hover,img[src="/pics/arrow3.gif"]:hover {background:#050305 url(http:///pouringthemilkshake.org/images/douban/deviantART&gt;Zilla/flame_block_z.gif) repeat scroll left bottom !important;}
}





