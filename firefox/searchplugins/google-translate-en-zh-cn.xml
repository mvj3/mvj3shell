<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Translate en-&gt;zh-CN</os:ShortName>
<os:Description>Translate english text to chinese</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/google_en2cn.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/google_en2cn.jpg</IconUpdateUrl>
<SearchForm>http://translate.google.com/translate_t</SearchForm>
<os:Url type="text/html" method="GET" template="http://translate.google.com/translate_t?hl=en&amp;ie=UTF-8&amp;text={searchTerms}&amp;langpair=en|zh-CN&amp;sourceid=Mozilla-search">
</os:Url>
</SearchPlugin>