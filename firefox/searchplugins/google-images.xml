<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Images</os:ShortName>
<os:Description>Google Images Search</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/googleimages.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/googleimages.ico</IconUpdateUrl>
<SearchForm>http://images.google.com/images</SearchForm>
<os:Url type="text/html" method="GET" template="http://images.google.com/images?q={searchTerms}&amp;hl=en&amp;sourceid=mozilla-search&amp;num=20&amp;start=0&amp;start=0">
</os:Url>
</SearchPlugin>